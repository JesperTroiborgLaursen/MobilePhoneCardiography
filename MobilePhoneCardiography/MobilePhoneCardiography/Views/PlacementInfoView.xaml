<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MobilePhoneCardiography.Views.PlacementInfoView"
             xmlns:vm="clr-namespace:MobilePhoneCardiography.ViewModels"
             xmlns:models="clr-namespace:MobilePhoneCardiography.Models;assembly=MobilePhoneCardiography"
             Title="{Binding Title}">
    
    <ContentPage.BindingContext>
        <vm:MeasureViewModel />
    </ContentPage.BindingContext>
    
    <ContentPage.Resources>
        <ResourceDictionary>
            <Color x:Key="Accent">#96d1ff</Color>
        </ResourceDictionary>
    </ContentPage.Resources>

   
    <Frame BackgroundColor="AliceBlue">
        <StackLayout>
        </StackLayout>
    </Frame>

</ContentPage>
